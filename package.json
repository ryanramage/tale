{
  "name": "tale",
  "version": "0.0.0",
  "description": "A location story telling platform",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/tape tests/*.js",
    "start": "cd html; ../node_modules/.bin/jam compile --no-license  -i js/app -o js/tale.min.js; cd ..; node server.js",
    "devel": "npm run-script unminify; npm run-script web;",
    "web": "node server.js",
    "bundle": "cd html; ../node_modules/.bin/jam compile --no-minify --no-license  -i js/app -o js/tale.min.js",
    "minify": "cd html; ../node_modules/.bin/jam compile --no-license  -i js/app -o js/tale.min.js",
    "unminify": "rm html/jam/tale.min.js",
    "browserify": "./node_modules/.bin/browserify lib/client/xxtea-decrypt.js  --standalone xxtea  > html/jam/xxtea.js"
  },
  "author": "Ryan Ramage",
  "license": "BSD-2-Clause",
  "dependencies": {
    "xxtea-stream": "0.0.0",
    "async": "^0.2.10",
    "lodash": "^2.4.1",
    "node-uuid": "^1.4.1",
    "level-store": "^3.10.1",
    "ls-stream": "^1.0.0",
    "sublevel": "^2.4.0",
    "bops": "^0.1.1",
    "sjcl": "^1.0.0",
    "express": "git://github.com/blakeembrey/express.git#modular-routing",
    "mime": "^1.2.11",
    "filereader-stream": "^0.1.0",
    "level": "^0.18.0",
    "mkdirp": "^0.3.5"
  },
  "devDependencies": {
    "memdown": "^0.7.0",
    "levelup": "^0.18.2",
    "concat-stream": "^1.4.3",
    "browserify": "^3.32.1"
  },
  "jam": {
    "baseUrl": "html",
    "packageDir": "html/jam"
  }
}
