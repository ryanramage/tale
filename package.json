{
  "name": "tale",
  "version": "0.0.0",
  "description": "A location story telling platform",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/tape tests/*.js",
    "start":  "cd html; ../node_modules/.bin/jam compile --no-license  -i js/app -o jam/require.prod.js; cd ..; node lib/web.js",
    "devel": "npm run-script unminify; npm run-script web;",
    "web": "node lib/web",
    "bundle": "cd html; ../node_modules/.bin/jam compile --no-minify --no-license  -i js/app -o jam/require.prod.js",
    "unminify": "rm html/jam/require.prod.js"
  },
  "author": "Ryan Ramage",
  "license": "BSD-2-Clause",
  "dependencies": {
    "xxtea-stream": "0.0.0",
    "async": "^0.2.10",
    "lodash": "^2.4.1",
    "node-uuid": "^1.4.1",
    "level-store": "^3.10.1",
    "ls-stream": "^1.0.0",
    "sublevel": "^2.4.0",
    "bops": "^0.1.1",
    "sjcl": "^1.0.0",
    "express": "git://github.com/blakeembrey/express.git#modular-routing"
  },
  "devDependencies": {
    "memdown": "^0.7.0",
    "levelup": "^0.18.2",
    "concat-stream": "^1.4.3"
  },
  "jam": {
    "baseUrl": "html",
    "packageDir": "html/jam"
  }
}
